{
    "plugins": ["jsdoc-mermaid", "../node_modules/@ckeditor/jsdoc-plugins/lib/export-fixer/export-fixer.js"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "default": {
            "includeDate": false
        },
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
